# This is a extended json package definition file 
# We allow simple comments (lines starting with a hash symbol)
{
    # This is currently our "master" repo set with versions
    # that we are trying to align other distributions with.
    # The versions should at least be major + minor!
    "ubuntu-oneiric": {
        "python-prettytable": {
            "version": "0.5*",
            "removable": true
        },
        "python-argparse": {
            "version": "1.1*",
            "removable": true
        }
    },
    "debian-wheezy": {
        "python-prettytable": {
            "version": "0.5-1",
            "allowed": ">=",
            "removable": true
        },
        # Not needed unless less than python 2.6 (not our case)
        #"python-simplejson": {
        #    "version": "2.1.6-1",
        #    "allowed": ">=",
        #    "removable": true
        #},
        "python-argparse": {
            "version": "1.1-1",
            "allowed": ">=",
            "removable": true
        }
    },
    "rhel-6": {
        # TBD
    }
}
